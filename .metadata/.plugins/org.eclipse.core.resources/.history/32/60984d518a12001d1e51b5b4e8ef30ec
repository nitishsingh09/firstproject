package numbers;

import java.util.Scanner;

public class Armstrong {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
		int temp = num;
		int count=0;
		
		
		while(temp>0) {
			count++;
			temp=temp/10;
		}
		System.out.println(count);
	}
}
